            android:name=".Actividades.PrincipalActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Actividades.CuentaActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Actividades.DatosPersonalesActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Actividades.MisTarjetasActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Actividades.PrivacidadActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Actividades.UbicacionActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Actividades.PagoActivity"
            android:theme="@style/AppTheme" />
